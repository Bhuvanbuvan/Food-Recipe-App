<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/suplierfoods"
    app:startDestination="@id/foodListFragment">

    <fragment
        android:id="@+id/foodListFragment"
        android:name="com.example.foodrecipe.fragment.FoodListFragment"
        android:label="fragment_food_list"
        tools:layout="@layout/fragment_food_list" >
        <action
            android:id="@+id/action_foodListFragment_to_addnewFoodFragment"
            app:destination="@id/addnewFoodFragment" />
        <action
            android:id="@+id/action_foodListFragment_to_updateFoodFragment"
            app:destination="@id/updateFoodFragment" />
    </fragment>
    <fragment
        android:id="@+id/addnewFoodFragment"
        android:name="com.example.foodrecipe.fragment.AddnewFoodFragment"
        android:label="fragment_addnew_food"
        tools:layout="@layout/fragment_addnew_food" />
    <fragment
        android:id="@+id/updateFoodFragment"
        android:name="com.example.foodrecipe.fragment.UpdateFoodFragment"
        android:label="fragment_update_food"
        tools:layout="@layout/fragment_update_food" />
</navigation>